{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Instance with holdings and items (there will be all instance properties, they just omitted)",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Instance id"
    },
    "holdings": {
      "type": "array",
      "description": "Holdings records for the instance",
      "items": {
        "$ref": "holdingsrecord.json"
      }
    },
    "items": {
      "type": "array",
      "description": "Items for the instance",
      "items": {
        "$ref": "item.json"
      }
    }
  },
  "additionalProperties": true
}

